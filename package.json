{
  "name": "@wya/socket",
  "version": "1.0.3",
  "description": "wya-socket for js",
  "main": "lib/index.js",
  "files": [
    "src/**",
    "lib/**",
    "dist/**",
    "examples/**"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run prompt && npm run all",
    "serve": "node --max_old_space_size=8192 ./node_modules/webpack-dev-server/bin/webpack-dev-server --config  ./config/webpack.config.dev.js --color --progress --hot",
    "demo": "cross-env MODE_ENV=build npm run prompt &&npm run clean && cross-env UGLIFY_JS=true node --max_old_space_size=8192 ./node_modules/webpack/bin/webpack --config ./config/webpack.config.dist.js --color --progress",
    "build": "rm -rf ./lib && cross-env NODE_ENV=production babel src --out-dir lib --copy-files --ignore **.test.js,**.md,examples/**",
    "pub": "bash config/publish.sh",
    "clean": "rm -rf ./dist ./demo",
    "lint": "esw ./config/*.config.* ./src/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --fix --watch",
    "lint:style": "stylelint --fix --syntax scss src",
    "mock": "cd ./src/mock && npm run start",
    "prompt": "node ./config/prompt.config.js",
    "all": "npm-run-all --parallel --race --continue-on-error lint:watch serve",
    "pre-commit:install": "cp .hooks/* .git/hooks/",
    "test": "jest --config ./tests/config/jest.config.js"
  },
  "author": "wya-team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wya-team/wya-socket"
  },
  "bugs": {
    "url": "https://github.com/wya-team/wya-socket/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-proposal-function-bind": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/runtime": "^7.8.7",
    "autoprefixer": "^9.6.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.4",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^6.8.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-markdown": "^1.0.1",
    "eslint-watch": "^6.0.1",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^5.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.0.4",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "^2.1.4",
    "postcss-smart-import": "^0.7.6",
    "precss": "^3.1.2",
    "prompt": "^1.0.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-webpack-plugin": "^1.0.2",
    "url-loader": "^2.2.0",
    "webpack": "~4.41.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-merge": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node ./config/commit-lint"
    }
  },
  "lint-staged": {
    "{src,config}/**/*.{js,jsx}": [
      "esw --fix",
      "git add"
    ],
    "*.md": [
      "esw --ext '.md' --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix --syntax scss",
      "git add"
    ]
  }
}
